{
  "name": "@yae/server",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "eslint .",
    "prepare": "bun run db:generate:agent && bun run db:generate:admin",
    "db:generate:agent": "drizzle-kit generate --config drizzle-agent.config.ts",
    "db:generate:admin": "drizzle-kit generate --config drizzle-admin.config.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@libsql/client": "latest",
    "@yae/baml": "workspace:*",
    "@yae/graph": "workspace:*",
    "agentfs-sdk": "latest",
    "drizzle-orm": "^0.45.1",
    "elysia": "^1.4.22",
    "elysia-rate-limit": "^4.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@standard-schema/spec": "^1.1.0",
    "drizzle-kit": "^0.31.8"
  }
}
