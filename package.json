{
  "name": "yae-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.5",
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "start:server": "turbo start --filter=@yae/server",
    "cli": "bun --filter=@yae/cli cli",
    "lint": "turbo lint",
    "format": "prettier . --write",
    "test": "turbo test"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "latest",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "3.7.4",
    "turbo": "^2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
